verbindungen
id	|	ip_adresse	|	browser_id	|	datum

google
verbindungen_id	|	tld		|	suchwort

browser
id	|	browser

pfade
verbindungen_id | seiten_id	|	zeit

seiten
id	|	seite

CREATE TABLE verbindungen(
id BIGINT AUTO_INCREMENT NOT NULL,
ip_adresse VARCHAR(15) NOT NULL,
browser_id INT,
datum INT,
PRIMARY KEY (id));

CREATE TABLE google(
verbindungen_id BIGINT NOT NULL,
tld VARCHAR(12),
suchwort VARCHAR(50),
PRIMARY KEY (verbindungen_id, tld, suchwort));

CREATE TABLE browser(
id INT AUTO_INCREMENT NOT NULL,
browser VARCHAR(200),
PRIMARY KEY (id));

CREATE TABLE pfade(
verbindungen_id BIGINT NOT NULL,
seiten_id INT NOT NULL,
zeit INT NOT NULL,
PRIMARY KEY (verbindungen_id, seiten_id, zeit));

CREATE TABLE seiten(
id INT AUTO_INCREMENT NOT NULL,
seite VARCHAR(20),
PRIMARY KEY (id));

CREATE TABLE events(
event_id INT AUTO_INCREMENT NOT NULL,
datum DATE NOT NULL,
va_name VARCHAR(50) NOT NULL,
va_ort VARCHAR(50),
sichtbar BOOL NOT NULL,
PRIMARY KEY (event_id));